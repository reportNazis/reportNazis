{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend-user": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/frontend-user",
      "sourceRoot": "projects/frontend-user/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/frontend-user",
            "index": "projects/frontend-user/src/index.html",
            "browser": "projects/frontend-user/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/frontend-user/tsconfig.app.json",
            "assets": [
              "projects/frontend-user/src/favicon.ico",
              "projects/frontend-user/src/assets"
            ],
            "styles": [
              "projects/frontend-user/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2000kb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2000kb",
                  "maximumError": "4mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "frontend-user:build:production"
            },
            "development": {
              "buildTarget": "frontend-user:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "frontend-user:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/frontend-user/tsconfig.spec.json",
            "assets": [
              "projects/frontend-user/src/favicon.ico",
              "projects/frontend-user/src/assets"
            ],
            "styles": [
              "projects/frontend-user/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "frontend-admin": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/frontend-admin",
      "sourceRoot": "projects/frontend-admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/frontend-admin",
            "index": "projects/frontend-admin/src/index.html",
            "browser": "projects/frontend-admin/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/frontend-admin/tsconfig.app.json",
            "assets": [
              "projects/frontend-admin/src/favicon.ico",
              "projects/frontend-admin/src/assets"
            ],
            "styles": [
              "projects/frontend-admin/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "baseHref": "/admin/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2000kb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2000kb",
                  "maximumError": "4mb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/frontend-admin/src/environments/environment.ts",
                  "with": "projects/frontend-admin/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "frontend-admin:build:production"
            },
            "development": {
              "buildTarget": "frontend-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "frontend-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/frontend-admin/tsconfig.spec.json",
            "assets": [
              "projects/frontend-admin/src/favicon.ico",
              "projects/frontend-admin/src/assets"
            ],
            "styles": [
              "projects/frontend-admin/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "shared-lib": {
      "projectType": "library",
      "root": "projects/shared-lib",
      "sourceRoot": "projects/shared-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/shared-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}